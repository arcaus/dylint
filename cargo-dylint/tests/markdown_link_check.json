{
  "httpHeaders": [
    { "urls": ["https://crates.io"], "headers": { "Accept": "text/html" } },
    { 
      "urls": ["https://github.com"], 
      "headers": { 
        "Accept": "text/html",
        "Authorization": "token ${GITHUB_TOKEN}"
      } 
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^https://[^/]*\\.openai\\.com/"
    }
  ]
}
