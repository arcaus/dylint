{
  "httpHeaders": [
    { "urls": ["https://crates.io"], "headers": { "Accept": "text/html" } },
    { 
      "urls": ["https://github.com", "https://api.github.com"],
      "headers": { 
        "Accept": "text/html",
        "Authorization": "token ${GITHUB_TOKEN}"
      }
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^https://[^/]*\\.openai\\.com/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^https://github.com/([^/]+)/([^/]+)/blob/(.+)",
      "replacement": "https://api.github.com/repos/$1/$2/contents/$3"
    },
    {
      "pattern": "^https://github.com/([^/]+)/([^/]+)$",
      "replacement": "https://api.github.com/repos/$1/$2"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s"
}
